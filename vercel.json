{
  "rewrites": [
    { "source": "/portal", "destination": "/index.html" },
    { "source": "/portal/(.*)", "destination": "/index.html" },
    { "source": "/form", "destination": "/index.html" },
    { "source": "/form/(.*)", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    },
    {
      "source": "/manifest.json",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ]
}
